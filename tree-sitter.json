{
  "grammars": [
    {
      "name": "solidity",
      "camelcase": "Solidity",
      "scope": "source.solidity",
      "path": ".",
      "file-types": [
        "sol"
      ],
      "injection-regex": "^(solidity|sol)$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Solidity grammar for tree-sitter",
    "authors": [
      {
        "name": "Bjerg"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-solidity"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
