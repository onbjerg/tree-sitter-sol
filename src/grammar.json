{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "solidity",
  "word": "identifier",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_source_element"
      }
    },
    "_source_element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "pragma_directive"
        },
        {
          "type": "SYMBOL",
          "name": "import_directive"
        }
      ]
    },
    "pragma_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "pragma"
        },
        {
          "type": "SYMBOL",
          "name": "pragma_token"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "pragma_token": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "pragma_name"
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "pragma_value"
          }
        }
      ]
    },
    "pragma_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_-]*"
    },
    "pragma_value": {
      "type": "PATTERN",
      "value": "[^;]+"
    },
    "import_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "import"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "string_literal"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "string_literal"
                },
                {
                  "type": "SYMBOL",
                  "name": "import_alias"
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "import_star"
                },
                {
                  "type": "SYMBOL",
                  "name": "import_alias"
                },
                {
                  "type": "STRING",
                  "value": "from"
                },
                {
                  "type": "SYMBOL",
                  "name": "string_literal"
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "import_clause"
                },
                {
                  "type": "STRING",
                  "value": "from"
                },
                {
                  "type": "SYMBOL",
                  "name": "string_literal"
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "import_star": {
      "type": "STRING",
      "value": "*"
    },
    "import_clause": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "import_name"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "import_name"
                      }
                    ]
                  }
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": ","
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "import_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "import_alias"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "import_alias": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "as"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "string_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "PATTERN",
                "value": "[^\"\\\\]|\\\\.|\\\\\\r?\\n"
              }
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "PATTERN",
                "value": "[^'\\\\]|\\\\.|\\\\\\r?\\n"
              }
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "//"
                },
                {
                  "type": "PATTERN",
                  "value": "[^\\r\\n]*"
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "/*"
                },
                {
                  "type": "PATTERN",
                  "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
                },
                {
                  "type": "STRING",
                  "value": "/"
                }
              ]
            }
          ]
        }
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_$][a-zA-Z0-9_$]*"
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "[\\s\\uFEFF\\u2060\\u200B\\u00A0]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
